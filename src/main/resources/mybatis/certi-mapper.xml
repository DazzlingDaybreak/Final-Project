<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Certi">
	<select id = "certiCount" resultType = "int">
		select count(*) from certi where refNickname = #{nickname} and chalSeq = #{chalSeq}
	</select>
</mapper>